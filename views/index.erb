<%= flash[:notice]%>

<ul>
<% @bookmarks.each do |url,title| %>
<li><a href=<%="#{url}"%>><%= title %></a></li>
<% end %>
</ul>

<form action="/bookmarks/new" method="get">
<input type="submit" value="Add Bookmark" style="width:350px;height:180px;"/>
</form>

<form action="/bookmarks/remove" method"get">
  <input type="submit" value="Remove Bookmark" style="width:350px;height:180px;"/>
</form>
